  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>

 </head>
  <body>

    <div class="navbar navbar-default navbar-static-top">
      <div class="container">
        <%= render  'layouts/header' %>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="col-lg-10">
          <%= bootstrap_flash %>
          <%= yield %>
        </div>
        <div class="col-lg-3">
          <%#= render :partial => 'layouts/right_side_bar' %>
        </div><!--/span-->
      </div><!--/row-->

      <%= render "layouts/footer" %>

    </div> <!-- /container -->

  </body>
</html>